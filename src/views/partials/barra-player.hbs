<html lang="es">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="css/barra-player.css" />

    <style>
      ._is-main-bar{ --primary-color: #f26419; --secondary-color: #0078aa;
      --tertiary-color: #0078aa; } ._is-main-bar-progress__current{ width: 100%;
      } ._is-main-bar__container{ color: #fff; }
    </style>

  </head>
  <body>
    <div class="_is-main-bar js-infiny-bar">
      <div class="_is-main-bar__overlay"></div>
