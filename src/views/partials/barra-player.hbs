          <div class="_is-main-bar__card _is-main-bar-card">
            <div class="_is-main-bar-card__left">
              <div class="_is-main-bar-card__media">
                <img src="img/logo.png" alt="logo" />
              </div>
            </div>
          </div>
        </div>
        <div class="_is-main-bar__center _is-main-bar__item">
          <!--<a href="#" class="_is-main-bar__button is-icon-rewind"></a>-->
          <audio controls autoplay="true">
            <source
              src="http://audio.streaminghd.cl:9134/stream?type=http&amp;nocache=15"
              type="audio/mp3"
            />
          </audio>
        </div>
      </div>
    </div>
